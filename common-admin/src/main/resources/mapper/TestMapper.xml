<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.common.system.mapper.TestMapper">

    <select id="queryall" resultType="com.common.system.entity.Test">
        select * from test
    </select>
    <select id="queryone" resultType="com.common.system.entity.Test" parameterType="int">
        select * from test where id=#{id}
    </select>

</mapper>
